lemma lem_Cf_nonzero_off_K
    {R R1 : ‚Ñù} {hR1_pos : 0 < R1} {hR1_lt_R : R1 < R} {hR_lt_1 : R < 1}
    {f : ‚ÑÇ ‚Üí ‚ÑÇ}
    {h_f_analytic : ‚àÄ z ‚àà Metric.closedBall (0 : ‚ÑÇ) 1, AnalyticAt ‚ÑÇ f z}
    {h_f_nonzero_at_zero : f 0 ‚â† 0}
