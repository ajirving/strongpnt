-- Lemma 30: logDeriv_numerator_pre
lemma logDeriv_numerator_pre {R : ‚Ñù} {œÅ : ‚ÑÇ} {z : ‚ÑÇ} :
    logDeriv (fun w ‚Ü¶ R - (star œÅ) * w / R) z = -(star œÅ) / R / (R - (star œÅ) * z / R) := by
  classical
  -- Put the function in the linear form b + a * w
