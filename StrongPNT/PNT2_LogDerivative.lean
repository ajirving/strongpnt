  exact lem_mod_Bf_at_0_eval R R1 hR1_pos hR1_lt_R hR_lt_1 f h_f_analytic h_f_nonzero_at_zero h_finite_zeros h_œÉ h_œÉ_spec

lemma lem_prod_ineq {Œπ : Type*} (K : Finset Œπ) (a b : Œπ ‚Üí ‚Ñù)
