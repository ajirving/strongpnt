  -- Show that Bf ... 0 ‚â† 0 using Bf_never_zero
  have h0_in_ball : (0 : ‚ÑÇ) ‚àà Metric.closedBall (0 : ‚ÑÇ) R1 := by
    simp [Metric.mem_closedBall, dist_zero_right]
