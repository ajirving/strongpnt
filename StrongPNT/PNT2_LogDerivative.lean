  let b : ‚ÑÇ := (R : ‚ÑÇ)
  have hlin : (fun w : ‚ÑÇ ‚Ü¶ (R : ‚ÑÇ) - (star œÅ) * w / (R : ‚ÑÇ)) = (fun w : ‚ÑÇ ‚Ü¶ b + a * w) := by
    funext w
    -- rewrite as b + a*w
    simp [a, b, sub_eq_add_neg, div_eq_mul_inv, mul_comm, mul_left_comm, mul_assoc,
          add_comm, add_left_comm, add_assoc]
  -- Compute the derivative of b + a * w
