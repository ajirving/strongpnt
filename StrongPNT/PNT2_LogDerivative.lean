      logDeriv (fun w ‚Ü¶ Bf R R1 hR1_pos hR1_lt_R hR_lt_1 f h_f_analytic (f_zero_ne_zero h_f_zero) h_finite_zeros h_œÉ w) z := by
  intro z _
