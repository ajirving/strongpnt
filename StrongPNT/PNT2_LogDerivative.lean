    (hR1_lt_R : R1 < R)
    (hR_lt_1 : R < 1)
    (f : ‚ÑÇ ‚Üí ‚ÑÇ)
    (h_f_analytic : ‚àÄ z ‚àà Metric.closedBall (0 : ‚ÑÇ) 1, AnalyticAt ‚ÑÇ f z)
    (h_f_zero : f 0 = 1)
    (h_finite_zeros : (zerosetKfR R1 (by linarith) f).Finite)
    (h_œÉ : ‚ÑÇ ‚Üí (‚ÑÇ ‚Üí ‚ÑÇ))
    (h_œÉ_spec : ‚àÄ œÉ ‚àà zerosetKfR R1 (by linarith) f,
