    (hf_le_B : ‚àÄ z : ‚ÑÇ, ‚Äñz‚Äñ ‚â§ R ‚Üí ‚Äñf z‚Äñ ‚â§ B) :
    ‚àÄ z : ‚ÑÇ, ‚Äñz‚Äñ = R ‚Üí
      ‚ÄñBf R R1 hR1_pos hR1_lt_R hR_lt_1 f h_f_analytic h_f_nonzero_at_zero h_finite_zeros h_œÉ z‚Äñ ‚â§ B := by
  -- proof body needs updating to use hR_lt_1
  intro z hz
