    (hf0_eq_one : f 0 = 1)
    (h_finite_zeros : (zerosetKfR R1 (by linarith) f).Finite)
    (h_œÉ : ‚ÑÇ ‚Üí (‚ÑÇ ‚Üí ‚ÑÇ))
    (hf_le_B : ‚àÄ z : ‚ÑÇ, ‚Äñz‚Äñ ‚â§ R ‚Üí ‚Äñf z‚Äñ ‚â§ B)
    (h_œÉ_spec : ‚àÄ œÉ ‚àà zerosetKfR R1 (by linarith) f,
